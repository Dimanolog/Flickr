<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">
    <servlet>
        <servlet-name>CheckVersionServlet</servlet-name>
        <servlet-class>com.github.dimanolog.flickr.backend.CheckVersionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckVersionServlet</servlet-name>
        <url-pattern>/checkVersion</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>